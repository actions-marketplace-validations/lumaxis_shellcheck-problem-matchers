{
  "problemMatcher": [
    {
      "owner": "shellcheck-json",
      "pattern": [
        {
          "regexp": "{\"file\":\"(.+?)\",\"line\":(\\d+).*\"column\":(\\d+).*\"level\":\"(style|info|warning|error)\",\"code\":(\\d+),\"message\":\"(.+)\".+},",
          "file": 1,
          "line": 2,
          "column": 3,
          "severity": 4,
          "code": 5,
          "message": 6
        }
      ]
    }
  ]
}
